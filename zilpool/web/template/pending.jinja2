{% extends "base.jinja2" %}
{% block body %}
{{ super() }}
<section class="text-white text-center">
  <div class="container">
    <div class="row mx-auto mt-5">
      {% if approved %}
      <div class="col-12 alert alert-success">Approved {{ pub_key }}</div>
      {% elif error %}
      <div class="col-12 alert alert-danger">{{ error }}</div>
      {% else %}
      <div class="col-12 alert alert-danger">Rejected {{ pub_key }}</div>
      {% endif %}
    </div>
  </div>
</section>
{% endblock %}
